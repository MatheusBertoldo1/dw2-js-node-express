<%- include("partials/header.ejs") %>


<div>
<h1>Lista de clientes</h1>

<table class="table table-bordered table-hover">
    <thead class="table-dark">
        <tr>
            <th>Nome</th>
            <th>Idade</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        <% clientes.forEach( cliente => {%>
        <tr>
            <td><%= cliente.nome %></td>
            <td><%= cliente.idade %></td>
            <td><%= cliente.email %></td>
        </tr>
        <% }) %>
    </tbody>
</table>
</div>

<%- include("partials/footer.ejs") %>
